<view class="width-container">
	<!-- 头像栏 -->
	<view class="include-wallet">
		<view class="mind-container">
			<!-- 左侧头像 -->
			<view class="top-user-container">
				<view class="top-user-pic-container">
					<image src="http://t8.baidu.com/it/u=1484500186,1503043093&fm=79&app=86&f=JPEG?w=1280&h=853" bindtap="chooseImage"></image>
				</view>
				<view>
					<view>{{ name }}</view>
					<view>{{renzheng}}</view>
				</view>
			</view>
			<!-- 邀请好友 -->
			<view>
				<button class="btn yqbtn">邀请好友</button>
			</view>
		</view>

		<view class="wallet-bgcolor">
			<!-- 钱包和券 -->
			<view class="wallet-container">
				<!-- 金额 -->
				<view>
					<view>{{ wallet }}</view>
					<view>总金额(元)</view>
				</view>
				<!-- 券 -->
				<view>
					<view>2</view>
					<view>发放中</view>
				</view>
				<!-- 佣金 -->
				<view>
					<view>25</view>
					<view>已提佣</view>
				</view>
			</view>
		</view>
	</view>


	<!-- 我的邀请 -->
	<view class="my-invented">
		<text>我的邀请</text>
		<navigator>明细 <text class="iconfont icon-iconfontzhizuobiaozhun023127"></text></navigator>
	</view>

	<view class="yaoqing-container">
		<!-- 总邀请人数 -->
		<view>
			<view>{{inviteNum}}</view>
			<view>总邀请人数</view>
		</view>
		<!-- 右侧 -->
		<view class="yaoqing-container-right">
			<!-- 今日新增 -->
			<view>
				<view>0</view>
				<view>今日新增</view>
			</view>

			<!-- 昨日新增 -->
			<view>
				<view>0</view>
				<view>昨日新增</view>
			</view>
		</view>
	</view>

	<view class="list-container">
		<!-- 我的优惠券 -->
		<view>
			<navigator url="./ticket/ticket">
				<view>我的优惠券</view>
				<text class="iconfont icon-iconfontzhizuobiaozhun023127"></text>
			</navigator>
			<!-- <navigator></navigator> -->
		</view>
		<!-- 我的收藏 -->
		<view>
			<navigator url="./myCollection/myCollection">
				<view>我的收藏</view>
				<text class="iconfont icon-iconfontzhizuobiaozhun023127"></text>
			</navigator>
		</view>
		<!-- 我的资料 -->
		<view>
			<navigator url="./userInfo/userInfo">
				<view>我的资料</view>
				<text class="iconfont icon-iconfontzhizuobiaozhun023127"></text>
			</navigator>
		</view>
		<!-- 我的消息 -->
		<view>
			<navigator url="./msg/msg">
				<view>我的消息</view>
				<text class="iconfont icon-iconfontzhizuobiaozhun023127"></text>
			</navigator>
		</view>
	</view>
</view>